{
    "name": "gstrackme",
    "version": "2.0.1",
    "description": "GSTrackme desktop application",
    "main": "src/server.js",
    "scripts": {
        "sp": "cross-env NODE_ENV=production electron src/server.js",
        "test": "mocha",
        "start": "electron-forge start",
        "package-win": "electron-forge package --arch=ia32 --platform=win32",
        "package-mac": "electron-forge package --arch=x64 --platform=darwin",
        "package-linux": "electron-forge package --arch=x64 --platform=linux",
        "make": "electron-forge make",
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "publish": "build --win -p always"
    },
    "author": "Juan X.",
    "license": "ISC",
    "devDependencies": {
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "electron-builder": "^20.38.5",
        "electron-forge": "^5.2.4",
        "electron-prebuilt-compile": "3.0.13",
        "mocha": "^6.0.1",
        "webpack": "^4.29.6"
    },
    "dependencies": {
        "asar": "^0.14.6",
        "axios": "^0.18.0",
        "bootstrap": "^4.2.1",
        "cross-env": "^5.2.0",
        "datatables.net": "^1.10.19",
        "datatables.net-dt": "^1.10.19",
        "electron-compile": "^6.4.4",
        "electron-debug": "^2.0.0",
        "electron-log": "^3.0.1",
        "electron-settings": "^3.2.0",
        "electron-squirrel-startup": "^1.0.0",
        "electron-updater": "^4.0.6",
        "express": "^4.16.4",
        "jquery": "^3.3.1",
        "node-notifier": "^5.3.0",
        "opn": "^5.4.0",
        "popper.js": "^1.14.6",
        "push.js": "^1.0.9",
        "socket.io": "^2.2.0",
        "sweetalert": "^2.1.2",
        "update-electron-app": "^1.3.0"
    },
    "config": {
        "forge": {
            "make_targets": {
                "win32": [
                    "squirrel"
                ],
                "darwin": [
                    "dmg"
                ],
                "linux": [
                    "deb",
                    "rpm"
                ]
            },
            "electronPackagerConfig": {
                "asar": true,
                "quiet": true,
                "icon": "./src/assets/images/icons/icon",
                "name": "GSTrackme",
                "productName": "GSTrackme"
            },
            "electronWinstallerConfig": {
                "loadingGif": "./src/assets/images/icons/loading.gif",
                "setupIcon": "./src/assets/images/icons/icon.ico",
                "packageName": "GSTrackme",
                "name": "GSTrackme",
                "productName": "GSTrackme"
            },
            "electronInstallerDMG": {
                "icon": "./src/assets/images/icons/icon.icns"
            },
            "electronInstallerDebian": {},
            "electronInstallerRedhat": {},
            "github_repository": {
                "owner": "",
                "name": ""
            },
            "windowsStoreConfig": {
                "packageName": ""
            }
        }
    },
    "build": {
        "appId": "com.GSTrackme",
        "win": {
            "target": "nsis",
            "icon": "./src/assets/images/icons/icon.ico"
        },
        "nsis": {
            "installerIcon": "./src/assets/images/icons/icon.ico",
            "uninstallerIcon": "./src/assets/images/icons/icon.ico",
            "runAfterFinish": true
        },
        "publish": [{
            "provider": "github",
            "owner": "jxool",
            "repo": "spiderProject"
        }]
    }
}